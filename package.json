    "d3-selection": "^1.4.0",
    "dayjs": "^1.11.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
