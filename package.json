{
  "name": "@cfpb/ccdb5-ui",
  "version": "2.15.4",
  "packageManager": "yarn@4.12.0",
  "description": "Consumer Complaint Database UI",
  "homepage": "https://www.consumerfinance.gov/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/ccdb5-ui.git"
  },
  "license": "CC0-1.0",
  "engines": {
    "node": ">=24.x"
  },
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "email": "tech@cfpb.gov",
    "url": "https://cfpb.github.io/"
  },
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "yarn prettier \"src/**/*.{js,jsx,ts,tsx,json,md}\" --write && yarn eslint ./src --fix && yarn stylelint \"src/**/*.{css,scss}\" --fix && yarn lint:cypress",
    "lint:cypress": "yarn eslint ./cypress --fix",
    "prepare": "husky",
    "release": "yarn run build && release-it --npm.skipChecks",
    "test": "TZ=UTC jest --coverage --watchAll=false",
    "start": "rsbuild dev",
    "build": "./scripts/build.sh",
    "preview": "rsbuild preview"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/plugin-transform-modules-commonjs": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@cfpb/cfpb-design-system": "^4.3.2",
    "@eslint/js": "^9.39.2",
    "@reduxjs/toolkit": "^2.11.2",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rsbuild/plugin-sass": "^1.5.0",
    "@rsbuild/plugin-svgr": "^1.3.0",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "britecharts": "git+https://github.com/cfpb/cfpb-britecharts.git#v2.5",
    "coveralls-next": "^6.0.1",
    "cypress": "^15.9.0",
    "d3": "^7.9.0",
    "dayjs": "^1.11.19",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^59.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-redux": "^4.2.2",
    "eslint-plugin-testing-library": "^7.15.4",
    "globals": "^16.5.0",
    "highcharts": "11.4.8",
    "history": "^5.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "3.0.0",
    "intro.js": "^7.2.0",
    "intro.js-react": "1.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.7",
    "lodash": "^4.17.23",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prop-types": "^15.8.1",
    "query-string": "^9.3.1",
    "react": "^19.2.4",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-typeahead": "^6.4.1",
    "react-dom": "^19.2.4",
    "react-fast-compare": "^3.2.2",
    "react-modal": "^3.16.3",
    "react-redux": "^9.2.0",
    "react-router": "^7.13.0",
    "release-it": "^19.2.4",
    "sass": "^1.97.3",
    "string-replace-loader": "^3.3.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0"
  },
  "lint-staged": {
    "src/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "{cypress,src}/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "{cypress,src}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
